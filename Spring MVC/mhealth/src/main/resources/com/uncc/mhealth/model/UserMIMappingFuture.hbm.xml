<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.uncc.mhealth.model">
    <class name="UserMIMappingFuture" table="user_mi_mapping_future">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="mi_id" column="mi_id" />
        <property name="user_id" column="user_id" />
        <property name="future_time" column="future_time" />
        <property name="pendingStatus" column="pendingStatus" />
    </class>
</hibernate-mapping>