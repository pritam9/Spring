<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.uncc.mhealth.model">
    <class name="ProsCons" table="pros_cons">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="user_id" column="user_id" />
        <property name="pros_cons" column="pros_cons" />
        <property name="updated_time" column="updated_time" />
    </class> 
</hibernate-mapping>