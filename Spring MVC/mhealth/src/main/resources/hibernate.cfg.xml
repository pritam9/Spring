<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>       
  <session-factory>
    <property name="dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="show_sql">true</property> 
    <mapping resource="com/uncc/mhealth/model/User.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/Question.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/Option.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/SubQuestion.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/UserSurvey.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/UserSurveyResponse.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/MI.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/MIOption.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/TriviaQuestion.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/TriviaOption.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/TriviaScore.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/Feedback.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/UserMIMapping.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/UserMIMappingFuture.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/UserToken.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/Strategies.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/BacLog.hbm.xml"/>
    <mapping resource="com/uncc/mhealth/model/ProsCons.hbm.xml"/>
  </session-factory>
</hibernate-configuration>