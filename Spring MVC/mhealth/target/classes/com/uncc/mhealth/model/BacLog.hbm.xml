<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.uncc.mhealth.model">
    <class name="BacLog" table="bac_log">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="user_id" column="user_id" />
        <property name="drinks" column="drinks" />
        <property name="bac" column="bac" />
        <property name="log_date" column="log_date" />
        <property name="beer" column="beer" />
        <property name="wine" column="wine" />
        <property name="shots" column="shots" />
        <property name="liquor" column="liquor" />
        
    </class> 
</hibernate-mapping>